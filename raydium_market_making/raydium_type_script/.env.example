## Copy this file to .env and fill in your values
# Base58-encoded secret key of your wallet (Keypair)
SECRET_KEY=<YOUR_BASE58_SECRET_KEY>

# Solana RPC URL (e.g. from QuickNode, Alchemy, or devnet/mainnet)
RPC_URL=<YOUR_RPC_URL>
  
# Wix Data API credentials
WIX_API_KEY=<YOUR_WIX_API_KEY>
WIX_DAILY_TDG_BUYBACK_ITEM_ID=<YOUR_WIX_DAILY_TDG_BUYBACK_ITEM_ID>
WIX_DATA_COLLECTION_ID=<YOUR_WIX_DATA_COLLECTION_ID>
WIX_ACCOUNT_ID=<YOUR_WIX_ACCOUNT_ID>
WIX_SITE_ID=<YOUR_WIX_SITE_ID>

# (Optional) USDC mint address (defaults to mainnet USDC)
USDC_MINT=<YOUR_USDC_MINT_ADDRESS>